function [mat_equiv] = matTimeConversion(txt_time)
 %incoming data is a datetime object not string so conversion is necessary
 %we have '[2017-10-04_08_29_35]'
 %we want 25-Sep-2017 06:42:31
str_elements = strsplit(mat_time,'_'); %seperate date from time
%gives [2017-10-04 08 29 35

%TIME
h=str_elements{1,2};
min=str_elements{1,3};
secs=str_elements{1,4};
time=strcat(h,':',min,':', secs);

%DATE
date=str_elements{1,1}(2:end);
date_elements = strsplit(date,'-');
day=date_elements{1,3};
m=date_elements{1,2};
y=date_elements{1,1};
da



%DATE
date=str_elements{1,1};
%convert month to number equivalent
m=month(str_elements{1,1});
if(length(num2str(m))==1) 
    m=strcat('0',num2str(m)); 
end
dcompo=strsplit(date,'-'); %split date up in day/month/year
day=dcompo{1,1}; year=dcompo{1,3};
date_reorg=strcat(year,'-',num2str(m),'-',day);

%TIME
time=str_elements{1,2};
tcompo=strsplit(time,':'); %split time up in hour/min/sec
hour=tcompo{1,1}; min=tcompo{1,2}; secs=tcompo{1,3};
time_reorg=strcat(hour,'_',min,'_',secs);

%Full
txt_equiv=strcat('[',date_reorg,'_',time_reorg,']');

end